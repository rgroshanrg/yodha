<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
  <div id="chartContainer" style="height: 300px; width: 100%;"></div>
  <div id="chartContainer1" style="height: 300px; width: 100%;"></div>
  <div id="chartContainer2" style="height: 300px; width: 100%;"></div>
  
  <div id="divchart">
    <span id="a"><%=a%></span>
    <span id="b"><%=b%></span>
    <span id="c"><%=c%></span>
    <span id="uc"><%=uc%></span>
    <span id="vc"><%=vc%></span>
    <span id="cc"><%=cc%></span>
  </div>

  <script>
      window.onload = function() {
        // notBought = Number(document.getElementById('notBought'));
        // boughtFree = Number(document.getElementById('boughtFree'));
        // boughtPaid = Number(document.getElementById('boughtPaid'));
        // console.log(notBought);
        // console.log(boughtFree);
        // console.log(boughtPaid);
        a = document.getElementById('a');
        b = document.getElementById('b');
        c = document.getElementById('c');
        a = Number(a.textContent);
        b = Number(b.textContent);
        c = Number(c.textContent);
        total = a+b+c;
        a = (100*a)/total;
        b = (100*b)/total;
        c = (100*c)/total;

        uc = Number(document.getElementById('uc').textContent);
        vc = Number(document.getElementById('vc').textContent);
        cc = Number(document.getElementById('cc').textContent);

        vc = (100*vc)/uc;
        cc = (100*cc)/uc;

        var chart = new CanvasJS.Chart("chartContainer", {
            animationEnabled: true,
            title: {
                text: "Inventory Buying Chart"
            },
            data: [{
                type: "pie",
                startAngle: 240,
                yValueFormatString: "##0.00\"%\"",
                indexLabel: "{label} {y}",
                dataPoints: [
                    {y: a , label: "Bought Free"},
                    {y: b, label: "Bought Paid"},
                    {y: c, label: "Bought Both"}
                ]
            }]
        });
        var chart1 = new CanvasJS.Chart("chartContainer1", {
            animationEnabled: true,
            title: {
                text: "Vaccination Requests Chart"
            },
            data: [{
                type: "pie",
                startAngle: 240,
                yValueFormatString: "##0.00\"%\"",
                indexLabel: "{label} {y}",
                dataPoints: [
                    {y: vc , label: "Requested"},
                    {y: (100-vc), label: "Not Requested"}
                ]
            }]
        });
        var chart2 = new CanvasJS.Chart("chartContainer2", {
            animationEnabled: true,
            title: {
                text: "Covid Testing Requests Chart"
            },
            data: [{
                type: "pie",
                startAngle: 240,
                yValueFormatString: "##0.00\"%\"",
                indexLabel: "{label} {y}",
                dataPoints: [
                    {y: cc , label: "Requested"},
                    {y: (100-cc), label: "Not Requested"}
                ]
            }]
        });
        chart.render();
        chart1.render();
        chart2.render();

    }
  </script>
    <script src="https://canvasjs.com/assets/script/canvasjs.min.js"></script>
</body>
</html>